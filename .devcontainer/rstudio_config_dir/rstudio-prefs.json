{
    "save_workspace": "never",
    "load_workspace": false,
    "auto_append_newline": true,
    "font_size_points": 11,
    "editor_theme": "Monokai",
    "posix_terminal_shell": "bash",
    "default_project_location": "~/rlib_projects",
    "initial_working_directory": "~",
    "use_spaces_for_tab": false,
    "num_spaces_for_tab": 4,
    "show_invisibles": true,
    "show_indent_guides": true,
    "scroll_past_end_of_document": true,
    "rainbow_parentheses": true,
    "default_encoding": "UTF-8",
    "syntax_color_console": true,
    "server_editor_font_enabled": true,
    "server_editor_font": "Fira Code",
    "source_with_echo": true,
    "jobs_tab_visibility": "shown",
    "panes": {
        "quadrants": [
            "Source",
            "Console",
            "TabSet1",
            "TabSet2",
            "HiddenTabSet"
        ],
        "tabSet1": [
            "Environment",
            "History",
            "Connections",
            "Build",
            "VCS",
            "Tutorial",
            "Presentation"
        ],
        "tabSet2": [
            "Files",
            "Plots",
            "Packages",
            "Help",
            "Viewer"
        ],
        "hiddenTabSet": [],
        "console_left_on_top": false,
        "console_right_on_top": true,
        "additional_source_columns": 1
    }
}